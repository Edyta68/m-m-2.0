{% extends 'base.html.twig' %}

{% block body %}
    <h1>SaleInvoice list</h1>

	<table class="table table-striped" >
        <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">Contractorid</th>
                <th scope="col">Fileid</th>
                <th scope="col">Taxid</th>
                <th scope="col">Title</th>
                <th scope="col">Number</th>
                <th scope="col">Amountnetto</th>
                <th scope="col">Amountbrutto</th>
                <th scope="col">Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for saleInvoice in saleInvoices %}
            <tr>
                <td scope="row"><a href="{{ path('saleinvoice_show', { 'id': saleInvoice.id }) }}">{{ saleInvoice.id }}</a></td>
                <td>{{ saleInvoice.contractorId }}</td>
                <td>{{ saleInvoice.fileId }}</td>
                <td>{{ saleInvoice.taxId }}</td>
                <td>{{ saleInvoice.title }}</td>
                <td>{{ saleInvoice.number }}</td>
                <td>{{ saleInvoice.amountNetto }}</td>
                <td>{{ saleInvoice.amountBrutto }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('saleinvoice_show', { 'id': saleInvoice.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('saleinvoice_edit', { 'id': saleInvoice.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('saleinvoice_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
